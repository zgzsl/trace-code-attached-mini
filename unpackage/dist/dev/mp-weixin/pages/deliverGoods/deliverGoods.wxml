<view class="good"><block wx:if="{{$root.m0===0}}"><view class="deliverGoods"><view class="context"><view class="header"><view class="left"><text>统计:</text><text>{{"共"+codeArr.length+"件货物"}}</text></view><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="btn" bindtap="__e">扫一扫</view></view><view class="content"><block wx:for="{{codeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{codeArr.length>0}}"><view class="item"><text>子码编号：</text><text>{{item}}</text><icon type="cancel" size="26" data-event-opts="{{[['tap',[['deleteCode',[index]]]]]}}" bindtap="__e"></icon></view></block></block><block wx:if="{{codeArr.length===0}}"><view class="item" style="color:rgba(128,128,128,1);">暂无关联的子码</view></block></view></view></view></block><block wx:if="{{$root.m1===1}}"><view class="deliverFinish"><view class="context"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><view class="context_item"><radio style="margin-left:10px;" color="rgb(246, 64, 70)" value="0" checked="{{0===current}}"></radio><view class="title">代理商：</view><picker class="picker" range-key="agentName" mode="selector" value="{{index}}" range="{{List}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e">{{''+List[index].agentName+''}}</picker></view><view class="context_item"><radio style="margin-left:10px;" color="rgb(246, 64, 70)" value="1" checked="{{1===current}}"></radio><view class="title">输入自定义商家：</view><input type="text" placeholder="直接输入商家名称" data-event-opts="{{[['input',[['__set_model',['','sellerName','$event',[]]]]]]}}" value="{{sellerName}}" bindinput="__e"/></view></radio-group></view></view></block><view class="btn_box"><block wx:if="{{active===1}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">上一步</button></block><block wx:if="{{active===0}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['next',['$event']]]]]}}" bindtap="__e">下一步</button></block><block wx:if="{{active===1}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['nextConfirm',['$event']]]]]}}" bindtap="__e">完成</button></block></view></view>