.deliverFinish .context {
  margin: 10px 10px;
}
.deliverFinish .context .context_item {
  border-radius: 10px;
  border: 1px solid #ccc;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 14px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 10px 0;
}
.deliverFinish .context .context_item .title {
  padding: 20rpx 10rpx;
  font-size: 24rpx;
}
.deliverFinish .context .context_item input {
  width: calc(100vw - 240rpx);
}
.deliverFinish .context .context_item .picker {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.deliverGoods {
  padding: 30rpx 30rpx 0;
}
.deliverGoods .context {
  overflow-y: hidden;
  border: 1px solid #ccc;
  border-radius: 10px;
}
.deliverGoods .context .header {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #ffffff;
  padding: 10px 5px;
  background: -webkit-linear-gradient(right, #f53647, #fd973c);
  background: linear-gradient(to left, #f53647, #fd973c);
  font-size: 30rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.deliverGoods .context .header .left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.deliverGoods .context .header .btn {
  color: #F53747;
  border-radius: 5px;
  border: 1px solid #fff;
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  color: #000;
  background: #fff;
}
.deliverGoods .context .content {
  overflow-y: scroll;
  height: calc(100vh - 400rpx);
  font-size: 14px;
  background: #f2f2f2;
  margin: 10px;
}
.deliverGoods .context .content .item {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 10px 2px;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}

