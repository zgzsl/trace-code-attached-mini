<view class="relation"><block wx:if="{{!showError}}"><view class="right"><block wx:if="{{outCode}}"><view class="out"><view class="context"><view class="header"><view class="left"><text>外码编号:</text><text>{{outCode||""}}</text></view><view data-event-opts="{{[['tap',[['relation',['$event']]]]]}}" class="btn" bindtap="__e">关联子码</view></view><view class="content"><block wx:for="{{chlidCodeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{chlidCodeArr.length>0}}"><view class="item"><view style="font-size:26rpx;">子码编号:</view><text style="font-size:26rpx;">{{item}}</text><icon type="cancel" size="26" data-event-opts="{{[['tap',[['deleteCode',[index]]]]]}}" bindtap="__e"></icon></view></block></block><block wx:if="{{chlidCodeArr.length===0}}"><view class="item" style="color:rgba(128,128,128,1);">暂无关联的子码</view></block></view></view><view class="btn_box"><button class="btn" type="primary" data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">完成</button></view></view></block><block wx:if="{{!outCode}}"><view class="outbtn"><button data-event-opts="{{[['tap',[['createdClick',['$event']]]]]}}" bindtap="__e">扫一扫获取外码</button></view></block></view></block><block wx:if="{{showError}}"><error vue-id="1" text="{{text}}" type="{{type}}" data-event-opts="{{[['^relation',[['relation']]],['^createdClick',[['createdClick']]]]}}" bind:relation="__e" bind:createdClick="__e" bind:__l="__l"></error></block></view>