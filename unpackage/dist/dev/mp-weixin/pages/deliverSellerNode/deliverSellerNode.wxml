<view class="good"><block wx:if="{{!showError}}"><view class="box"><block wx:if="{{$root.m0===0}}"><view class="deliverGoods"><view class="context"><view class="header"><view class="left">列表</view><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="btn" bindtap="__e">添加编号</view></view><view class="content"><block wx:for="{{codeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{codeArr.length>0}}"><view class="item" style="position:relative;"><text style="font-size:26rpx;">子码编号：</text><text style="font-size:26rpx;">{{item.traceSubCodeNumber||""}}</text><icon type="cancel" size="26" data-event-opts="{{[['tap',[['deleteCode',[index]]]]]}}" bindtap="__e"></icon><text style="position:absolute;left:86rpx;bottom:-5px;">{{"sid : "+item.traceSid}}</text></view></block></block><block wx:if="{{codeArr.length===0}}"><view class="item" style="color:rgba(128,128,128,1);">暂无编号</view></block></view></view></view></block><block wx:if="{{$root.m1===1}}"><view class="deliverFinish"><view class="context"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><view class="title_header">模式</view><view class="context_item"><radio style="margin-left:10px;" color="rgb(246, 64, 70)" value="0" checked="{{0===current}}"></radio><view class="title">已经出售</view></view><view class="context_item"><radio style="margin-left:10px;" color="rgb(246, 64, 70)" value="1" checked="{{1===current}}"></radio><view class="title">添加节点</view></view><block wx:if="{{1===current}}"><view><view class="title_header">节点信息</view><view class="context_item"><view class="title">手机号码：</view><input type="text" placeholder="输入手机号码" data-event-opts="{{[['input',[['__set_model',['','contactNumber','$event',[]]]]]]}}" value="{{contactNumber}}" bindinput="__e"/></view><view class="context_item"><view class="title">追溯点名称：</view><input type="text" disabled="{{sellerNameSelect}}" placeholder="输入名称" data-event-opts="{{[['input',[['__set_model',['$0','tracePointName','$event',[]],['otherMerchant']]]]]}}" value="{{otherMerchant.tracePointName}}" bindinput="__e"/></view><view class="context_item"><view class="title">负责人：</view><input type="text" disabled="{{userNameSelect}}" placeholder="输入名称" data-event-opts="{{[['input',[['__set_model',['$0','personInCharge','$event',[]],['otherMerchant']]]]]}}" value="{{otherMerchant.personInCharge}}" bindinput="__e"/></view></view></block></radio-group></view></view></block><view class="btn_box"><block wx:if="{{active===1}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">上一步</button></block><block wx:if="{{active===0}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['next',['$event']]]]]}}" bindtap="__e">下一步</button></block><block wx:if="{{active===1}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['nextConfirm',['$event']]]]]}}" bindtap="__e">完成</button></block></view></view></block><block wx:if="{{showError}}"><error vue-id="1" text="{{text}}" type="{{type}}" data-event-opts="{{[['^scanCode',[['scanCode']]]]}}" bind:scanCode="__e" bind:__l="__l"></error></block></view>