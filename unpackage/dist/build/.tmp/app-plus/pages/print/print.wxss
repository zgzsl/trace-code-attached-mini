}
.print .setting_number_methods_input input {
  border-bottom: 1px solid #ccc;
  -webkit-box-flex: 0.8;
  -webkit-flex: 0.8;
          flex: 0.8;
}
.print .setting_number_methods_input image {
  width: 40rpx;
  height: 40rpx;
}
.print_header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border: 1px solid #F53B47;
  width: calc(100vw - 80rpx);
  margin: 0 auto 0;
  border-radius: 50px;
  overflow: hidden;
}
.print_header .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  font-size: 13px;
  padding: 10px;
}
.print_header .item.active {
  color: #fff;
  background: -webkit-linear-gradient(right, #f53647, #fd973c);
  background: linear-gradient(to left, #f53647, #fd973c);
