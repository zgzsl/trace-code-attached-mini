.print {
  padding-bottom: 120rpx;
  padding-top: 15px;
}
.print ::-webkit-scrollbar {
  width: 0;
}
.print .scroll-Y {
  height: 100%;
  width: 100%;
  white-space: nowrap;
  padding: 15px;
}
.print .scroll-Y .nothing {
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.print .scroll-Y .nothing .nogood {
  width: 400rpx;
  height: 400rpx;
  margin: 20% auto 0;
}
.print .scroll-Y .nothing .nogood image {
  width: 100%;
  height: 100%;
}
.print .scroll-Y .nothing text {
  font-size: 14px;
}
.print .scroll-Y .scroll_item {
  margin-bottom: 15px;
  font-size: 13px;
}
.print .btn_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: fixed;
  bottom: 0;
  width: 100%;
}
.print .btn_box .btn {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #fff;
  background: -webkit-linear-gradient(right, #f53647, #fd973c);
  background: linear-gradient(to left, #f53647, #fd973c);
  line-height: 2;
  border-radius: 49px;
  margin: 10px 15px;
  padding: 5px 0;
  font-size: 13px;
}
.print .code_step {
  font-size: 12px;
  color: #CCCCCC;
  padding: 10px 10px 0;
}
.print .concent {
  font-size: 30rpx;
  padding: 10px 15px;
}
.print .uni-badge-success {
  background: #fff !important;
  color: black !important;
}
.print .one_step .uni-list_one .item {
  justify-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 28rpx;
  padding: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 1px solid #ccc;
}
.print .one_step .uni-list_one .item .right {
  color: #CCCCCC;
}
.print .one_step .list {
  padding: 0 15px;
}
.print .one_step .list .zsNot {
  text-align: center;
  font-size: 28rpx;
  padding: 10px;
  color: #CCCCCC;
}
.print .one_step .list .list_item {
  padding: 5px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px solid #ccc;
  font-size: 24rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.print .one_step .list .list_item radio {
  width: 40px;
}
.print .one_step .list .list_item text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.print .one_step .list .list_item .img {
  width: 30px;
  height: 30px;
}
.print .one_step .list .list_item .img image {
  width: 100%;
  height: 100%;
}
.print .scend_step .goodList {
  padding: 0 15px;
}
.print .scend_step .goodList .zsNot {
  text-align: center;
  font-size: 28rpx;
  padding: 10px;
  color: #CCCCCC;
}
.print .scend_step .goodList .item {
  border: 1px solid #CCCCCC;
  border-radius: 10px;
  overflow: hidden;
  margin: 15px 0;
}
.print .scend_step .goodList .item:first-child {
  margin: 0 0 15px;
}
.print .scend_step .goodList .item_header {
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 13px;
  background: #E6E6E6;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
}
.print .scend_step .goodList .item_botom {
  font-size: 13px;
  border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
