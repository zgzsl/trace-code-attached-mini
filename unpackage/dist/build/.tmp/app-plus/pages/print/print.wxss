.print .scend_step .goodList .item_botom input {
  width: 100px;
  border-bottom: 1px solid #CCCCCC;
}
.print .third_step .img {
  text-align: center;
  margin: 30px 0;
}
.print .third_step .img image {
  width: 120rpx;
  height: 120rpx;
}
.print .setting_number {
  padding: 0 15px;
}
.print .setting_number_methods_detail {
  font-size: 14px;
  padding: 0 0 10px 0;
  color: #ccc;
}
.print .setting_number_methods {
  font-size: 16px;
  padding: 10px 0;
}
.print .setting_number_methods_input {
  border: 1px solid #cccccc;
  border-radius: 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 14px;
  padding: 10px 2px;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.print .setting_number_methods_input text {
  width: 40px;
