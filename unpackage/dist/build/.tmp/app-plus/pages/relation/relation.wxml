<view class="relation"><block wx:if="{{!showError}}"><view class="right"><block wx:if="{{outCode}}"><view class="out"><view class="context"><view class="header"><view class="left"><text>外码编号:</text><text>{{outCode||""}}</text></view><block wx:if="{{!deliverGoods}}"><view data-event-opts="{{[['tap',[['relation',['$event']]]]]}}" class="btn" bindtap="__e">关联子码</view></block><block wx:if="{{deliverGoods}}"><view class="btn">已发货</view></block></view><view class="{{['content1',(deliverGoods)?'contentMax':'']}}"><mix-tree vue-id="1" list="{{list}}" deliverGoods="{{deliverGoods}}" data-event-opts="{{[['^treeItemClick',[['treeItemClick']]],['^deleteCode',[['deleteCode']]]]}}" bind:treeItemClick="__e" bind:deleteCode="__e" bind:__l="__l"></mix-tree></view></view><view class="btn_box"><block wx:if="{{!deliverGoods}}"><button class="btn" type="primary" data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">完成</button></block></view></view></block><block wx:if="{{!outCode}}"><view class="outbtn"><button data-event-opts="{{[['tap',[['createdClick',['$event']]]]]}}" bindtap="__e">扫一扫获取外码</button><view style="font-size:26rpx;margin:15rpx 0;">温馨提醒：如果机器是扫码枪，使用机器自带按钮或者点击屏幕按钮方可使用</view></view></block></view></block><block wx:if="{{showError}}"><error vue-id="2" text="{{text}}" type="{{type}}" data-event-opts="{{[['^relation',[['relation']]],['^createdClick',[['createdClick']]]]}}" bind:relation="__e" bind:createdClick="__e" bind:__l="__l"></error></block></view>